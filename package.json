{
  "name": "youtube-trends-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Ahd-Kabeer-Hadi/youtube-trends-api.git",
  "homepage": "https://github.com/Ahd-Kabeer-Hadi/youtube-trends-api.git/README.md",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "serve": "node dist/server.js",
